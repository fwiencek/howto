{
  "name": "howto",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "analyze": "cross-env ANALYZE=true yarn build",
    "build": "next build",
    "cms:dev": "netlify-cms-proxy-server",
    "create:favicons": "ts-node -r tsconfig-paths/register -P scripts/tsconfig.json scripts/favicons",
    "create:rss": "ts-node -r tsconfig-paths/register -P scripts/tsconfig.json scripts/rss",
    "create:sitemap": "ts-node -r tsconfig-paths/register -P scripts/tsconfig.json scripts/sitemap",
    "debug": "cross-env NODE_OPTIONS='--inspect' next dev",
    "dev": "next dev",
    "export": "next export",
    "format": "prettier . -l --ignore-path .gitignore",
    "format:fix": "yarn format --write",
    "lint": "eslint . --ignore-path .gitignore",
    "lint:fix": "yarn lint --fix",
    "postbuild": "yarn create:sitemap",
    "prebuild": "rimraf .next && yarn create:favicons",
    "preexport": "rimraf out",
    "start": "next start",
    "storybook:build": "build-storybook -s public,src/styles -c stories",
    "storybook:dev": "start-storybook -p 6006 -s public,src/styles -c stories --ci --quiet",
    "test": "jest",
    "test:coverage": "yarn test --coverage",
    "test:watch": "yarn test --watch"
  },
  "dependencies": {
    "@next/bundle-analyzer": "^10.0.9",
    "@react-aria/i18n": "^3.3.0",
    "@react-aria/overlays": "^3.6.1",
    "@react-aria/progress": "^3.1.1",
    "@react-aria/ssr": "^3.0.1",
    "@react-stately/overlays": "^3.1.1",
    "@stefanprobst/next-error-boundary": "^1.0.7",
    "@stefanprobst/next-mdx": "^1.2.2",
    "@stefanprobst/next-page-metadata": "^1.0.9",
    "@stefanprobst/next-svg": "^1.1.3",
    "@stefanprobst/rehype-shiki": "^0.1.1",
    "@stefanprobst/remark-excerpt": "^1.0.2",
    "@stefanprobst/remark-extract-yaml-frontmatter": "^1.0.1",
    "clsx": "^1.1.1",
    "gray-matter": "^4.0.2",
    "hast-util-has-property": "^1.0.4",
    "hast-util-heading-rank": "^1.0.1",
    "hast-util-to-string": "^1.0.4",
    "hastscript": "^6.0.0",
    "next": "^10.0.9",
    "nprogress": "^0.2.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-toastify": "^7.0.3",
    "rehype-autolink-headings": "^5.0.1",
    "rehype-slug": "^4.0.1",
    "rehype-stringify": "^8.0.0",
    "remark-footnotes": "^3.0.0",
    "remark-frontmatter": "^3.0.0",
    "remark-gfm": "^1.0.0",
    "remark-parse": "^9.0.0",
    "remark-rehype": "^8.0.0",
    "remark-stringify": "^9.0.1",
    "rosetta": "^1.1.0",
    "strip-markdown": "^4.0.0",
    "unified": "^9.2.1",
    "use-composed-ref": "^1.1.0",
    "yaml": "^1.10.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.0.1",
    "@commitlint/config-conventional": "^12.0.1",
    "@next/eslint-plugin-next": "^10.0.9",
    "@next/plugin-storybook": "10.0.6-canary.8",
    "@stefanprobst/commitlint-config": "^1.0.1",
    "@stefanprobst/eslint-config-next": "^1.0.9",
    "@stefanprobst/eslint-config-next-component": "^1.0.10",
    "@stefanprobst/favicons": "^1.0.7",
    "@stefanprobst/next-sitemap": "^1.0.5",
    "@stefanprobst/prettier-config": "^1.0.1",
    "@storybook/addon-a11y": "^6.2.0-rc.5",
    "@storybook/addon-essentials": "^6.2.0-rc.5",
    "@storybook/react": "^6.2.0-rc.5",
    "@tailwindcss/jit": "^0.1.5",
    "@tailwindcss/typography": "^0.4.0",
    "@testing-library/cypress": "^7.0.4",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@testing-library/user-event": "^13.0.0",
    "@types/jest": "^26.0.20",
    "@types/next-auth": "^3.7.2",
    "@types/node": "^14.14.35",
    "@types/nprogress": "^0.2.0",
    "@types/react": "^17.0.3",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "autoprefixer": "^10.2.5",
    "cross-env": "^7.0.3",
    "cypress": "^6.7.1",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.1",
    "eslint-plugin-jest-dom": "^3.6.5",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-testing-library": "^3.10.1",
    "husky": "^4.3.8",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "jest-axe": "^4.1.0",
    "lint-staged": "^10.5.4",
    "netlify-cms-proxy-server": "^1.3.11",
    "postcss": "^8.2.8",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "start-server-and-test": "^1.11.7",
    "tailwindcss": "^2.0.3",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.3",
    "xast-util-feed": "^1.0.0",
    "xast-util-to-xml": "^2.0.1"
  },
  "engines": {
    "node": "14.x",
    "yarn": "^1.22.0"
  },
  "babel": {
    "presets": [
      "next/babel"
    ]
  },
  "commitlint": {
    "extends": [
      "@stefanprobst/commitlint-config"
    ]
  },
  "eslintConfig": {
    "extends": [
      "@stefanprobst/eslint-config-next",
      "plugin:jest-dom/recommended",
      "plugin:testing-library/react",
      "plugin:cypress/recommended"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "tsc && lint-staged"
    }
  },
  "lint-staged": {
    "*.(js|jsx|ts|tsx)": "eslint --cache --fix",
    "*.(css|json|md|mdx)": "prettier --write"
  },
  "prettier": "@stefanprobst/prettier-config"
}
